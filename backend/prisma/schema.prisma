generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Inspector {
  id        Int      @id @default(autoincrement())
  vehicleId Int
  date      DateTime
}

model Trip {
  routeId               Int
  service_id            Int
  trip_id               String @id
  trip_headsign         String
  direction_id          Int
  shape_id              Int
  wheelchair_accessible Int
  brigade               Int
}

model Shape {
  shape_id          Int   @id
  shape_pt_lat      Float
  shape_pt_lon      Float
  shape_pt_sequence Int
}
